generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  username String?
  hashedPassword String?
  email String? @unique
  emailVerified DateTime?
  image String?
  userType String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  favoriteIds String[]
}